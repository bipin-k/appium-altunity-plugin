    "watch": "tsc --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@appium/base-driver": "^8.2.4",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/ws": "^8.5.1",
    "@types/xmldom": "^0.1.31",
    "asyncbox": "^2.9.2",
    "jest": "^27.1.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "webdriverio": "^7.12.5"
